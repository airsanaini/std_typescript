{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "../dist",
    "rootDir": "../src",
    "esModuleInterop": true,
    "strict": true,
    "noEmitOnError": true,
    "skipLibCheck": true
  },
  "include": ["../src/**/*.ts"],
  "exclude": ["node_modules", "../dist"]  // Pastikan folder dist dikecualikan
}